{
  "name": "mdc/swagger-test",
  "description": "A set of tools for testing your REST calls based on the Swagger or OpenAPI documentation using PHPUnit",
  "version": "2.0.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "byjg/webrequest": "4.9.*",
    "php": ">=7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.5|^10.5",
    "byjg/restserver": "4.9.*"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\ApiTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
